class Setting():
    
    def __init__(self):
        self.__tick = 48
        self.__allowId = [0, 2, 4, 5]
    
    @property
    def tick(self):
        return self.__tick
    
    def isAllowed(self, id):
        for i in self.__allowId:
            if id == i:
                return True
        return False
    
def isNear(nx, ny, item):
    sets = Setting()
    if abs(nx - item.x) == sets.tick or abs(ny - item.y) == sets.tick:
        print(abs(nx - item.x))
        print(abs(ny - item.y))
        return True
    else:
        return False